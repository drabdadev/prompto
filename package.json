{
  "name": "webprompter",
  "version": "1.0.0",
  "description": "Prompt management webapp with Kanban UI",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "init-db": "node server/init-database.js",
    "install-all": "npm install && cd client && npm install",
    "test:server": "jest --config jest.server.config.js"
  },
  "keywords": ["prompts", "kanban", "management"],
  "author": "Michele Melis",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.6.0",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.7"
  }
}
